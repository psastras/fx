{
  "name": "fx",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/psastras/fx.git",
  "author": "Paul Sastrasinh <psastras@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack.prod.config.js",
    "lint": "tslint --project src",
    "start": "webpack-dev-server --config webpack.dev.config.js",
    "pretest": "webpack --config webpack.test.config.js",
    "test": "nyc ava --verbose",
    "posttest": "npm run lint"
  },
  "ava": {
    "files": [
      ".build/test/**/*.js"
    ],
    "require": [
      "source-map-support/register"
    ],
    "concurrency": 4
  },
  "nyc": {
    "exclude": [
      ".build"
    ]
  },
  "devDependencies": {
    "@types/node": "^7.0.5",
    "@types/react-router": "^3.0.0",
    "@types/three": "^0.83.1",
    "ava": "^0.18.1",
    "babel": "^6.5.2",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "file-loader": "^0.10.0",
    "glob": "^7.1.1",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.0",
    "nyc": "^10.1.2",
    "react-hot-loader": "next",
    "sass-loader": "^4.1.1",
    "source-map-support": "^0.4.11",
    "style-loader": "^0.13.1",
    "ts-loader": "^2.0.0",
    "tslint": "^4.4.2",
    "typescript": "^2.1.5",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "webpack-node-externals": "^1.5.4"
  },
  "dependencies": {
    "font-awesome": "^4.7.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-lite": "^0.15.31",
    "react-router": "^3.0.2",
    "three": "^0.84.0"
  }
}
