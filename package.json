    "@types/redux-logger": "^3.0.0",
    "@types/three": "^0.84.7",
    "ava": "^0.19.1",
    "babel": "^6.23.0",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
